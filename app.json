{
  "expo": {
    "name": "YÖKDİL Hazırlık",
    "slug": "yokdil-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yokdilapp",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F1E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F0F1E"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.qtes34.yokdilapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "shortName": "YÖKDİL",
      "display": "standalone",
      "themeColor": "#0F0F1E",
      "backgroundColor": "#0F0F1E"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "buildFeatures": {
              "buildConfig": true
            },
            "enableMultiDex": true
          }
        }
      ],
      "./plugins/withAndroidFix.js",
      "./plugins/withProguardRules.js",
      "./plugins/withBuildConfig.js",
      "./plugins/withDesugaring.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d84a6599-a05d-4f02-90a5-f27ef3c379f7"
      }
    }
  }
}